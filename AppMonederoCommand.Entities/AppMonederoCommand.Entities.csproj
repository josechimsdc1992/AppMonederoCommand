    </Compile>
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Update="Lenguajes\Menssages.es.resx">
      <LastGenOutput>Menssages.es.Designer.cs</LastGenOutput>
      <Generator>PublicResXFileCodeGenerator</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Lenguajes\Menssages.resx">
      <LastGenOutput>Menssages.Designer.cs</LastGenOutput>
      <Generator>PublicResXFileCodeGenerator</Generator>
    </EmbeddedResource>
