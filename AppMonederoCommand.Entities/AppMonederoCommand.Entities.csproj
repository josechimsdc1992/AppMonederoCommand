  <ItemGroup>
    <PackageReference Include="CsvHelper" Version="32.0.3" />
    <PackageReference Include="IMD.Utils" Version="1.0.1.6" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Pagos\Banorte\" />
